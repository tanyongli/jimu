@charst "utf-8"
html
  touch-action: manipulation
  
body
button
dd
dl
dt
footer
h1
h2
h3
h4
input
li
nav
ol
p
textarea
ul
  margin: 0
  padding: 0
  
body
  font: Montserrat, "Microsoft YaHei", "Lucida Sans Unicode", "Myriad Pro", "Hiragino Sans GB", "Heiti SC", Verdana, simsun
  -webkit-text-size-adjust: none
  min-width: 320px
  max-width: 750px
  margin: auto
  font-size: 12px
  font-family: Montserrat
  
@font-face 
  font-family: Montserrat
  src: url(/font/Montserrat-Light.eot)
  src: url(/font/Montserrat-Light.eot?#iefix) format('embedded-opentype'), url(/font/Montserrat-Light.woff) format('woff'), url(/font/Montserrat-Light.ttf) format('truetype'), url(/font/Montserrat-Light.svg#MontserratLight) format('svg')
  font-weight: 400
  font-style: normal

h1
h2
h3
h4
h5
h6 
  font-size: 100%
  
form 
	display: inline

ol
ul 
	list-style: none
a
  text-decoration: none
  color:#1a1a1a
  &:active
  &:focus
  &:hover
    color:#1c5aa2
    text-decoration: none

img 
  vertical-align: middle
  border: 0
  -ms-interpolation-mode: bicubic

button
input
select
textarea 
  font-size: 100%
  vertical-align: middle
  outline: 0

textarea 
  resize: none
     
input 
	background: 0 0


button
input[type=button]
input[type=reset]
input[type=submit]
  cursor:pointer
  vender(n,arg)
    -webkit-{n}:arg
    -moz-{n}:arg
  appearance()
    vender('appearance',arguments)
  appearance:button
  background: 0 0
input:focus:-moz-placeholder,
input:focus::-webkit-input-placeholder {
	color: transparent
}

button::-moz-focus-inner
input::-moz-focus-inner 
	padding: 0
	border: 0


table 
	border-collapse: collapse
	border-spacing: 0
	

.fl 
	float: left


.fr 
	float: right


.hide 
	display: none


.show 
	display: block


.ellipsis 
	white-space: nowrap
	text-overflow: ellipsis
	overflow: hidden


.break 
	word-break: break-all
	word-wrap: break-word


article
footer
header
hgroup
menu
nav
section 
	display: block
	clear: all


input[type=number] 
	-moz-appearance: textfield


.clearfix 
	clear: both


input::-webkit-inner-spin-button
input::-webkit-outer-spin-button 
	-webkit-appearance: none!important
	margin: 0


[v-cloak] 
	display: none

#jm-aboutus
  .jimu-logo
    margin: 1.0667rem 0
    display: block
    text-align: center
    img
      display: inline-block
      width: 159px
      width: 2.12rem
      height: 56px
      height: .7467rem
  span.label
      display: block
      text-align: center
      font-size: 20px
      padding-bottom: 1.0667rem
      position: relative
      &:after
        content: ' '
        width: 20px
        height: 3px
        position: absolute
        left: 0
        right: 0
        margin: 35px auto
        background: #3793df
  .features-wrap
    padding: 0 .6667rem 1.0667rem
    .label
      padding-bottom: 0
    .feature
      float: left
      text-align: center
      width: 50%
      img
        width: 60px
        margin: .8667rem auto .4667rem
      .tit
        color: #2c3038
        font-size: 17px
        margin: .1333rem auto
      p
        line-height: 18px
        color: #8b939f
  .choice
    background: url(https://m.jimu.com/images/aboutus/mountain-bg-fa6666d1ac.png) top center #3793df
    background-size: cover
    padding: 1.0667rem .6667rem 2.1333rem 
    box-sizing: border-box
    .tit
      text-align: center
      margin: .5333rem 0
      font-size: 30px
    span.label
        color: #fff
        margin-bottom: .6667rem
        &:after
          background: #fff
    table
      border: none
      tr
        display: block
        padding: .1333rem 0
        font-size: 12px
        color: #2c3038
        vertical-align: top
        td:first-child
          padding-right: 5px
          white-space: nowrap
          vertical-align: top
    .swiper-wrapper
      margin: 0 auto
      align-items: center
      -webkit-perspective: 3000
      -webkit-backface-visibility: hidden
    .swiper-slide
      padding: 0 .4rem .8rem
      box-sizing: border-box
      margin: 0 .16rem
      background: #fff
      vender(n,arg)
        -webkit-{n}:arg
        -moz-{n}:arg
        -ms-{n}:arg
        -o-{n}:arg
        {n}:arg
      box-shadow()
        vender('box-shadow',arguments)
      box-shadow: 0 0 60px 0 rgba(19, 105, 176, .8)
      vender(n,arg)
        -webkit-{n}:arg
        -moz-{n}:arg
        {n}:arg
      border-radius()
        vender('border-radius',arguments)
      border-radius: 8px
      height: 100%
      vender(n,arg)
        -webkit-{n}:arg
        -moz-{n}:arg
        -ms-{n}:arg
        -o-{n}:arg
        {n}:arg
      transform()
        vender('transform',arguments)
      transform: scale(1, .9)
      opacity: .8
    .swiper-slide-active
      vender(n,arg)
        -webkit-{n}:arg
        -moz-{n}:arg
        -ms-{n}:arg
        -o-{n}:arg
        {n}:arg
      transform()
        vender('transform',arguments)
      transform: scale(1, 1)
      vender(n,arg)
        -webkit-{n}:arg
        -moz-{n}:arg
        -ms-{n}:arg
        -o-{n}:arg
        {n}:arg
      transition()
        vender('transition',arguments)
      transition: transform
      opacity: 1
      z-index: 2
    .swiper-pagination
      bottom: 50px
      bottom: .6667rem
      .swiper-pagination-bullet
        opacity: .5
        background: #fff
      .swiper-pagination-bullet-active
        background: #fff
        opacity: 1
  .logo-history-wrap
    text-align: center
    padding: 1.0667rem .5333rem
    img
      width: 80px
      margin: .8rem auto .4rem
    p
      line-height: 18px
      text-align: left
      margin-bottom: .2667rem
    p.tit
        text-align: center
        font-size: 17px
    p.content
        text-align: center
        color: #8b939f
    
      
  
  
  
  
  
  
  
  
  
  
  
  
  